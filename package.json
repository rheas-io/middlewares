{
  "name": "@rheas/middlewares",
  "version": "0.0.5",
  "description": "Common middlewares used in the framework",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "rheas",
    "laravel",
    "express",
    "framework",
    "nodejs",
    "middlewares",
    "server",
    "backend",
    "api"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "copypackage": "cp -rf package.json build",
    "build": "tsc && npm run copypackage",
    "copypackage_dev": "cp -rf package.json build_dev",
    "build-dev": "tsc --outDir ./build_dev && npm run copypackage_dev"
  },
  "devDependencies": {
    "@rheas/contracts": "^0.0.5",
    "@rheas/errors": "^0.0.3",
    "@rheas/support": "^0.0.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rheas-io/middlewares.git"
  },
  "author": "Kalesh Kaladharan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rheas-io/middlewares/issues"
  },
  "homepage": "https://github.com/rheas-io/middlewares#readme"
}
